<?xml version="1.0" encoding="UTF-8"?>
<sequence name="fault_demo" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
    <log level="custom">
        <property name="text" value="FAULT_DEMO_SEQ: An unexpected error occured"/>
        <property expression="get-property('ERROR_MESSAGE')" name="message"/>
        <property expression="get-property('ERROR_DETAIL')" name="Details"/>
    </log>
    <drop/>
</sequence>
